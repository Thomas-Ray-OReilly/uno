AAA,GAAA,CAAA,IAAA,CAAA,SAAA,IAAA,EAAA,EAAA;AACA,YAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,SAAA;AACA;AACA;AACA,OAXA;AAYA,KAdA;AAeA,IAAA,SAfA,uBAeA;AAAA;;AACA,MAAA,KAAA,CACA,IADA,CACA,qCAAA,KAAA,OAAA,GAAA,GAAA,GAAA,KAAA,QADA,EAEA,IAFA,CAEA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OAJA;AAKA,KArBA;AAsBA,IAAA,QAtBA,oBAsBA,IAtBA,EAsBA;AAAA;;AACA,MAAA,KAAA,CACA,IADA,CAEA,gCACA,KAAA,OADA,GAEA,GAFA,GAGA,KAAA,QAHA,GAIA,GAJA,GAKA,IAAA,CAAA,MALA,GAMA,GANA,GAOA,IAAA,CAAA,KATA,EAWA,IAXA,CAWA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OAbA;AAcA,KArCA;AAsCA,IAAA,QAtCA,sBAsCA;AAAA;;AACA,UAAA,CAAA,GAAA,gCAAA,KAAA,OAAA,GAAA,GAAA,GAAA,KAAA,QAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,CAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,UAAA;AACA,OAHA;AAIA;AA7CA,GA1BA;AAyEA,EAAA,OAzEA,qBAyEA;AAAA;;AACA,IAAA,WAAA,CAAA,YAAA;AACA,MAAA,MAAA,CAAA,UAAA;AACA,KAFA,EAEA,IAFA,CAAA;AAGA;AA7EA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-container>\n      <v-row :class=\"'mb-6'\">\n        <v-col :cols=\"6\">\n          <!-- Game stats -->\n          <v-row>\n            <v-card :class=\"'ma-3 pa-6'\" outlined tile>\n              Current Game id: {{ game_id }}\n              <span v-if=\"!valid\">Invalid Game Id</span>\n            </v-card>\n            <v-card :class=\"'ma-3 pa-6'\" outlined tile>\n              <v-btn v-if=\"valid\" @click.native=\"startGame\">Start Game</v-btn>\n              <v-btn v-else to=\"/\">Create a new game</v-btn>\n            </v-card>\n          </v-row>\n\n          <!-- Game Players -->\n          <v-row>\n            <v-card\n              v-for=\"player in players\"\n              :key=\"player\"\n              color=\"current_player == player ? '#1F7087' : ''\"\n              :class=\"'ma-3 pa-6'\"\n              outlined\n              tile\n            >{{ player }}</v-card>\n          </v-row>\n\n          <!-- Current Card and actions -->\n          <v-col cols=\"12\">\n            <v-row v-if=\"current_player != ''\">\n              <v-card :class=\"'ma-3 pa-6'\" outlined tile class=\"center-text\">\n                <Card\n                  v-for=\"card in current_card\"\n                  :number=\"card.number\"\n                  :key=\"card.color\"\n                  :color=\"card.color\"\n                />\n              </v-card>\n            </v-row>\n          </v-col>\n        </v-col>\n\n        <!-- Current cards in the deck -->\n        <v-col :class=\"'mb-6'\" v-if=\"current_card != ''\">\n          <v-card\n            v-if=\"username == current_player\"\n            :class=\"'ma-3 pa-6'\"\n            outlined\n            tile\n          >\n            Click to play a card from your hand or\n            <v-btn v-if=\"username == current_player\" @click.native=\"drawCard\">Draw from deck</v-btn>\n          </v-card>\n          <v-card v-else-if=\"game_over != ''\">{{game_over}} has won the game!</v-card>\n\n          <v-card v-else :class=\"'ma-3 pa-6'\" outlined tile>Waiting for {{ current_player }}</v-card>\n          <Card\n            v-for=\"(card, i) in cards\"\n            :key=\"i\"\n            :number=\"card.number\"\n            :color=\"card.color\"\n            @click.native=\"playCard(card)\"\n          ></Card>\n        </v-col>\n      </v-row>\n    </v-container>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Card from \"../components/Card\";\nexport default {\n  props: {\n    game_id: {\n      required: false\n    },\n    valid: {\n      type: Boolean,\n      required: false\n    },\n    username: {\n      type: String,\n      required: false\n    }\n  },\n  data() {\n    return {\n      cards: [],\n      current_player: \"\",\n      players: [],\n      current_card: [],\n      game_over: \"\"\n    };\n  },\n  components: {\n    Card\n  },\n  methods: {\n    updateData() {\n      axios.get(\"http://localhost:8080/update/\" + this.game_id + \"/\" + this.username).then(res => {\n        if (res.data.valid) {\n          this.valid = res.data.valid;\n          this.cards = res.data.payload.deck;\n          this.current_player = res.data.payload.current_player;\n          this.players = res.data.payload.all_players;\n          this.current_card = res.data.payload.current_card;\n          if (res.data.game_over != \"\") {\n            this.game_over = res.data.game_over;\n          }\n        }\n      });\n    },\n    startGame() {\n      axios\n        .post(\"http://localhost:8080/startgame/\" + this.game_id + \"/\" + this.username)\n        .then(() => {\n          this.updateData();\n        });\n    },\n    playCard(card) {\n      axios\n        .post(\n          \"http://localhost:8080/play/\" +\n            this.game_id +\n            \"/\" +\n            this.username +\n            \"/\" +\n            card.number +\n            \"/\" +\n            card.color\n        )\n        .then(() => {\n          this.updateData();\n        });\n    },\n    drawCard() {\n      var f = \"http://localhost:8080/draw/\" + this.game_id + \"/\" + this.username;\n      console.log(f);\n      axios.post(f).then(res => {\n        console.log(res.data);\n        this.updateData();\n      });\n    }\n  },\n  created() {\n    setInterval(() => {\n      this.updateData();\n    }, 2000);\n  }\n};\n</script>"],"sourceRoot":"src/views"}]}